#!/usr/bin/env bash

function main {
  local ahome="$(unset CDPATH; cd -P -- "$(dirname -- "$BASH_SOURCE")/../.." && pwd -P)"
  local shome="$(unset CDPATH; cd -P -- "$(dirname -- "$BASH_SOURCE")/.." && pwd -P)"

  source "$shome/script/profile" || return 1
  source "${APP_PATH:-$ahome}/jq/script/profile" || return 1

  source app_bootstrap

  block_walker bootstrap "$@"
}

source sub "$0" "$@"
