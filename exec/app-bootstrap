#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"

  source "$shome/script/profile" || return 1

  {
    app runmany 'exec 2>&-; ls -d $1/script/bootstrap; ls -d $1/script/profile > /dev/null && echo source $1/script/profile' 
  } | bash -x -
}

source sub "$0" "$@"
