#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile" ''

  if [[ "$#" == 0 ]]; then
    return
  fi

  require
  if [[ -f Blockfile.json && ! -e .git ]]; then
    cd -P "$(git rev-parse --show-cdup)"
    source script/profile
    cd -
  fi
  eval "$@"
}

source sub "$0" "$@"
